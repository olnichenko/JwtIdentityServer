{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "DbSettings": {
    "ConnectionString": "Data Source=AspIdUsers.db;"
  },
  "TokenSettings": {
    "SecretKey": "MyTokenSecretKey",
    "Issuer": "JwtTokenServer",
    "ValidTokenHours": 168
  },
  "IdentitySettings": {
    "PasswordResetLinkExpirationDateHours": 72,
    "PasswordResetLinkUrl": "https://localhost:5001/Account/ResetPassword/"
  },
  "AllowedHosts": "*"
}
